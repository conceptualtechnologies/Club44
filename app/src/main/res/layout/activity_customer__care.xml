<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"

    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/app_bar"
        layout="@layout/app_bar"
        />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
    <LinearLayout
        android:id="@+id/layout_sign_up10"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        android:orientation="vertical"
        android:layout_marginTop="65dp">

        <android.support.v7.widget.CardView
            android:id="@+id/card_view_id_forgot3"
            android:layout_width="fill_parent"
            android:layout_height="150dp"
            android:background="@color/white"
            android:layout_marginBottom="2dp"
            android:layout_marginEnd="0dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="2dp"
            card_view:contentPadding="10dp"
            android:layout_marginStart="3dp">
            <LinearLayout
                android:id="@+id/layout_sign_up5"
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:layout_marginBottom="6dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tv_number3"
                    android:layout_width="160dp"
                    android:layout_height="35dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="15dp"

                    android:text="01204080551"
                    android:textColor="#161515"
                    android:textSize="18dp" />
                <TextView
                    android:id="@+id/tv_name3"
                    android:layout_width="160dp"
                    android:layout_height="35dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="15dp"

                    android:text="Direct Order"
                    android:textColor="#161515"
                    android:textSize="18dp" />

                <ImageView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/iv_message2"
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:src="@drawable/message"
                    android:layout_gravity="right"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="40dp" />

                <ImageView
                    android:id="@+id/iv_call2"
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="-60dp"
                    android:src="@drawable/call" />

                <ImageView
                    android:id="@+id/profile_imag2"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/user"
                    android:layout_gravity="left"
                    android:layout_marginTop="-90dp" />
            </LinearLayout>
        </android.support.v7.widget.CardView>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/layout_sign_up2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        android:orientation="vertical"
        android:layout_marginTop="225dp">

        <android.support.v7.widget.CardView
            android:id="@+id/card_view_id_forgot"
            android:layout_width="fill_parent"
            android:layout_height="150dp"
            android:background="@color/white"
            android:layout_marginBottom="2dp"
            android:layout_marginEnd="0dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="2dp"
            card_view:contentPadding="10dp"
            android:layout_marginStart="3dp">
            <LinearLayout
                android:id="@+id/layout_sign_up"
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:layout_marginBottom="6dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tv_number"
                    android:layout_width="160dp"
                    android:layout_height="35dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="15dp"

                    android:text="9818944886"
                    android:textColor="#161515"
                    android:textSize="18dp" />
                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="160dp"
                    android:layout_height="35dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="15dp"

                  android:text="Customer Care"
                    android:textColor="#161515"
                    android:textSize="18dp" />

                <ImageView
                    xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/iv_message"
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:src="@drawable/message"
                    android:layout_gravity="right"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="40dp" />

                <ImageView
                    android:id="@+id/iv_call"
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="-60dp"
                    android:src="@drawable/call" />

                <ImageView
                    android:id="@+id/profile_imag"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/user"
                    android:layout_gravity="left"
                    android:layout_marginTop="-90dp" />
            </LinearLayout>
        </android.support.v7.widget.CardView>

</LinearLayout>
    <LinearLayout
        android:id="@+id/layout_sign_up3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="6dp"
        android:orientation="vertical"
        android:layout_marginTop="385dp">

        <android.support.v7.widget.CardView
            android:id="@+id/card_view_id_forgot2"
            android:layout_width="fill_parent"
            android:layout_height="150dp"
            android:background="@color/white"
            android:layout_marginBottom="2dp"
            android:layout_marginEnd="0dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="2dp"
            card_view:contentPadding="10dp"
            android:layout_marginStart="3dp">
            <LinearLayout
                android:id="@+id/layout_sign_up4"
                android:layout_width="fill_parent"
                android:layout_height="120dp"
                android:layout_marginBottom="6dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tv_number2"
                    android:layout_width="160dp"
                    android:layout_height="35dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="15dp"

                    android:text="Email Id :"
                    android:textColor="#161515"
                    android:textSize="18dp" />

                <TextView
                    android:id="@+id/tv_name2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="100dp"
                    android:layout_marginRight="15dp"
                    android:text="         club44batch@gmail.com"
                    android:textColor="#161515"
                    android:textSize="18dp" />

                <ImageView
                    android:id="@+id/iv_email"
                    android:layout_width="85dp"
                    android:layout_height="55dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="-10dp"
                    android:src="@drawable/email" />



                <ImageView
                    android:id="@+id/profile_image"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:background="@drawable/user"
                    android:layout_gravity="left"
                    android:layout_marginTop="-100dp" />
            </LinearLayout>
        </android.support.v7.widget.CardView>

    </LinearLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
